/*******************************
         Site Overrides
*******************************/
.ui.seamless.segment {
  padding-top: 0px;
  padding-bottom: 0px;
}

.ui.borderless.segment {
  border-bottom: 0px;
}

.ui.lightgray.inverted.segment {
  background: @lightgray !important;
  color: @primaryColor !important;
}

.ui.border.segment {
  border-bottom: 1px solid @borderColor !important;
}

.ui.edge.segment {
  border-radius: 0em;
}
