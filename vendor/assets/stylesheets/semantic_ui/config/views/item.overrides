/*******************************
    User Variable Overrides
*******************************/
.ui.inverted.items {
  & > .item {
    & > .content {
      & > .header {
        color: @white;
      }

      & > .description {
        color: @invertedMutedTextColor;
      }

      a:not(.ui) {
        color: @invertedLightTextColor;
      }

      a:not(.ui):hover {
        color: @invertedTextColor;
      }
    }
  }
}
